  "version": "0.2.0",
  "configurations": [
    // launch config for debugging Vue web apps
    {
      "name": "Debug Nuxt: WebApp",
      "type": "msedge",
      "presentation": {
        "hidden": false,
        "group": "1 PROGRAM:NUXT",
        "order": 3
      },
      "request": "attach",
      "webRoot": "${workspaceFolder}",
    },

    {
      "name": "Debug Nuxt: Server",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "1 PROGRAM:NUXT",
        "order": 2
      },
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev", "--", "-o"]
    }
  ]
}